<?xml version="1.0" encoding="utf-8"?>
<com.ewgvip.buyer.android.layout.MyRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_gray">

    <ImageView
        android:id="@+id/iv_item_order_evaluate_photo"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="1dp"
        android:scaleType="fitXY" />

    <TextView
        android:id="@+id/tv_item_order_evaluate_photo"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_margin="8dp"
        android:background="@mipmap/checked_round" />
</com.ewgvip.buyer.android.layout.MyRelativeLayout>
