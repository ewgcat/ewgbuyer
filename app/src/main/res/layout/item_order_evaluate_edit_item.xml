<?xml version="1.0" encoding="utf-8"?>
<com.ewgvip.buyer.android.layout.MyRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fresco="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/light_gray">

    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/iv_item_order_evaluate_edit"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_centerInParent="true"
        android:padding="5dp"
        fresco:failureImage="@mipmap/ic_stub"
        fresco:failureImageScaleType="center"
        fresco:placeholderImage="@mipmap/ic_stub"
        fresco:placeholderImageScaleType="center"
        fresco:retryImage="@mipmap/ic_stub"
        fresco:retryImageScaleType="center"
        fresco:roundAsCircle="false" />

    <TextView
        android:id="@+id/tv_item_order_evaluate_photo_item_del"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_alignParentRight="true"
        android:background="@mipmap/fail" />
</com.ewgvip.buyer.android.layout.MyRelativeLayout>
